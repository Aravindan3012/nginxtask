"1	LINE	0
	name: Deploy Nginx to k3s
	
	on:
	  push:
	    branches:
	      - main
	
	jobs:
	  deploy:
	    runs-on: ubuntu-latest
	
	    steps:
	      - name: Checkout the repo
	        uses: actions/checkout@v3
	
	      - name: Set up Kubeconfig
	        run: |
	          mkdir -p ~/.kube
	          echo "${{ secrets.KUBECONFIG_DATA }}" | base64 -d > ~/.kube/config
	
	      - name: Deploy to k3s
	        run: |
	          kubectl apply -f k8s/
	
